[BITS 64]
	global my_rindex
	section .text

end:
	mov rax, rdx
	leave
	ret

store_addr:
	xor rdx, rdx
	mov rdx, rdi
	inc rdi
	jmp loop

loop:
	mov bl, byte[rdi]

	mov cl, sil

	cmp bl, 0
	jz end
	cmp cl, 0
	jz end
	cmp bl, cl
	je store_addr

	inc rdi
	jmp loop

my_rindex:
	push rbp
	mov rbp, rsp

	xor bl, bl
	xor cl, cl
	xor rax, rax
	xor rdx, rdx

	jmp loop
